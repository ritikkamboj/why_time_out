





// converting binary to decimal number 
#include <bits/stdc++.h>
using namespace std;

int main()
{
	int n1, n2;
	int arr[n1];
	cin>>n1>>n2;
	for(int i=0;i<n1;i++)
	{
		cin>>arr[i];

	}
	while(n2--)
	{
		int a1,a11,a12,a13;
		cin>>a1;
		if(a1==1)
		{
			cin>>a11;
			if(arr[a11-1]==0)
				arr[a11-1]=1;
			else
			{
				arr[a11-1]=0;
			}
				

		}
		else
		{
			if(a1==0)
			{
				cin>>a12>>a13;
				if(arr[a13-1]==1)
				cout<<"ODD";
				else
				{
					cout<<"EVEN";
				}
				
				
			}
		}
	}
	
}
